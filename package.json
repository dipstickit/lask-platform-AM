{
  "name": "lask-platform-admin",
  "version": "1.4.1",
  "scripts": {
    "openapi-gen": "openapi-generator-cli generate --input-spec=./path-to-your-openapi-spec.yaml --generator-name=typescript-angular --output=./src/app/core/api --reserved-words-mappings=return=return --additional-properties=fileNaming=kebab-case,serviceSuffix=ApiService,serviceFileSuffix=-api.service",
    "postinstall": "npm run openapi-gen",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint --fix",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/cdk": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/material": "^17.0.0",
    "marked": "^14.0.0",
    "ngx-markdown": "^15.0.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular-eslint/builder": "^17.0.0",
    "@angular-eslint/eslint-plugin": "^17.0.0",
    "@angular-eslint/eslint-plugin-template": "^17.0.0",
    "@angular-eslint/schematics": "^17.0.0",
    "@angular-eslint/template-parser": "^17.0.0",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.0",
    "@cypress/schematic": "^2.0.0",
    "@ngrx/eslint-plugin": "^16.0.0",
    "@ngrx/schematics": "^16.0.0",
    "@openapitools/openapi-generator-cli": "^2.5.1",
    "@types/jasmine": "~4.0.0",
    "@types/marked": "^4.0.8",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "cypress": "latest",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "~4.2.0",
    "jasmine-marbles": "^0.9.2",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "^2.7.1",
    "typescript": "^5.4.2"
  }
}
